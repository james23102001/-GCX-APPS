#SaveManga.gs
#register.gcs
#role : Insérer un utilisateur dans la base de données
connect database data.db
GetInput 0
SetVar *libelle LASTRESULT
GetEditText
SetVar *desc LASTRESULT
evaluate {*desc} [:-1]=> *desc
print var *desc
print var *userId
execute query insert
commit
MessageBox Congrats Manga cree!
create window app/screens/mangas.ch destroy current
end

[insert]
insert into Manga(libelle , description, idAuteur) values ( *libelle, *desc, *userId );
[/insert]