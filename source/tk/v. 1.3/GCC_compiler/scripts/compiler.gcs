#Command line script
define $WRITE scripts/createFile.gcs

SetVar *compiler C:\shortcuts\mingw_minimal\bin\gcc

split *fn ./1
SetVar *appname LASTRESULT

evaluate '@"'+{*compiler}+'" "'+{*fn}+'" -o "'+{*appname}+'"\n@"'+{*appname}+'"'

SetVar *FileText LASTRESULT
SetVar *Filename temp.bat
execute $WRITE 1

evaluate system('start temp.bat')
end